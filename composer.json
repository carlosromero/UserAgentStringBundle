{
    "name": "carlos-romero/useragentstring-bundle",
    "type": "symfony-bundle",
    "description": "Symfony2 bundle for integrating UserAgentString",
    "keywords": ["user agent", "detection"],
    "homepage": "https://github.com/andres-montanez/UserAgentStringBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrés Montañez",
            "email": "andres@andresmontanez.com",
            "homepage": "http://andresmontanez.com"
        }
    ],
    "replace": {
         "andres-montanez/useragentstring-bundle" : "*"
    }
    "require": {
        "php":                       ">=5.3.3",
        "symfony/framework-bundle":  "<=2.4.0"
    },
    "autoload": {
        "psr-0": { "AndresMontanez\\UserAgentStringBundle": "" }
    },
    "target-dir": "AndresMontanez/UserAgentStringBundle"
}
